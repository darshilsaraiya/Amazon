	<body class="hold-transition skin-blue sidebar-mini">
		<div class="wrapper">
			<header class="main-header">
				<% include ./partials/admin-header %>
			</header>

			<!-- Left side column. contains the logo and sidebar -->
			<aside class="main-sidebar">
				<!-- sidebar: style can be found in sidebar.less -->
				<section class="sidebar">
					<!-- Sidebar user panel -->
					<div class="user-panel">
						<div class="pull-left image">
							<img src="/dist/img/user2-160x160.jpg" class="img-circle" alt="User Image">
						</div>
						<div class="pull-left info">
							<p>Admin Name</p>
							<a href="#"><i class="fa fa-circle text-success"></i> Online</a>
						</div>
					</div>
					<!-- search form --><!--
						<form action="#" method="get" class="sidebar-form">
							<div class="input-group">
								<input type="text" name="q" class="form-control" placeholder="Search...">
								<span class="input-group-btn">
									<button type="submit" name="search" id="search-btn" class="btn btn-flat"><i class="fa fa-search"></i></button>
								</span>
							</div>
						</form>-->
					<!-- /.search form -->

					<!-- sidebar menu: : style can be found in sidebar.less -->
					<ul class="sidebar-menu">
						<li class="header">MAIN NAVIGATION</li>
						<li class="treeview">
							<a href="#">
								<i class="fa fa-dashboard"></i> <span>Dashboard</span> <i class="fa fa-angle-left pull-right"></i>
							</a>
							<ul class="treeview-menu">
								<li><a href="/admin/home"><i class="fa fa-circle-o"></i> Dashboard</a></li>
								<li><a href="/admin/home"><i class="fa fa-circle-o"></i> Statistics</a></li>
							</ul>
						</li>
						<li class="active treeview">
							<a href="#">
								<i class="fa fa-table"></i> <span>Tables</span>
								<i class="fa fa-angle-left pull-right"></i>
							</a>
							<ul class="treeview-menu">
								<li><a href="/admin/farmers/list"><i class="fa fa-circle-o"></i> Farmers List</a></li>
								<li class="active"><a href="/admin/products/list"><i class="fa fa-circle-o"></i> Products List</a></li>
								<li><a href="/admin/trucks/list"><i class="fa fa-circle-o"></i> Trucks List</a></li>
								<li><a href="/admin/drivers/list"><i class="fa fa-circle-o"></i> Drivers List</a></li>
								<li><a href="/admin/customers/list"><i class="fa fa-circle-o"></i> Customers List</a></li>
								<li><a href="/admin/orders/list"><i class="fa fa-circle-o"></i> Orders List</a></li>
							</ul>
						</li>
					</ul>
				</section>
				<!-- /.sidebar -->
			</aside>

			<!-- Content Wrapper. Contains page content -->
			<div ng-app="adminModule" ng-controller="adminController" ng-init="getproducts();" class="content-wrapper">
				<!-- Content Header (Page header) -->
				<section class="content-header">
					<h1>
						Product Data Tables
						<small>All data</small>
					</h1>
					<ol class="breadcrumb">
						<li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
						<li><a href="#">Tables</a></li>
						<li class="active">Products table</li>
					</ol>
				</section>

				<!-- Main content -->
				<section class="content">
					<div class="row">
						<div class="col-xs-12">
							<div class="box">
								<div class="box-header">
									<h3 class="box-title">Products Data Table</h3>
								</div><!-- /.box-header -->
								<div class="box-body">
									<table id="example1" class="table table-bordered table-hover table-striped">
										<thead>
											<tr>
												<th>Product Id</th>
												<th>Product Name</th>
												<th>Category Id</th>
												<th>Price</th>
												<th>Weight</th>
												<th>Average Ratings</th>
												<th>Product by Farmer</th>
												<th>Product Info</th>
												<th>Unit</th>
												<th>Updated At</th>
											</tr>
										</thead>
										<tbody>
											<tr ng-repeat="product in products">
												<td>{{product.p_id}}</td>
												<td>
													<span editable-text="product.name" e-name="name" e-form="productform" e-required>
														 {{ product.name || 'empty' }}
													</span>
												</td>
												<td>
													<span editable-text="product.cat_id" e-name="cat_id" e-form="productform" e-required>
														 {{ product.cat_id || 'empty' }}
													</span>
												</td>
												<td>
                          <span editable-text="product.price" e-name="price" e-form="productform" e-required>
                             {{ product.price || 'empty' }}
                          </span>
                        </td>
                        <td>
                          <span editable-text="product.weight" e-name="weight" e-form="productform" e-required>
                             {{ product.weight || 'empty' }}
                          </span>
                        </td>
                       	<td>{{product.ratings || 'empty'}}</td>
												<td>{{product.byfarmer || 'empty'}}</td>
												<td>
                          <span editable-text="product.details" e-name="details" e-form="productform" e-required>
                             {{ product.details || 'empty' }}
                          </span>
                        </td>
                        <td>
                          <span editable-text="product.unit" e-name="unit" e-form="productform" e-required>
                             {{ product.unit || 'empty' }}
                          </span>
                        </td>
												<td>{{product.updatedAt | date: 'medium' || 'empty'}}</td>

												<td>
										      <form editable-form name="productform" ng-show="productform.$visible" class="form-buttons form-inline"
										            shown="inserted == product" onbeforesave="saveProduct($data,product.p_id)">
										        <button type="submit" ng-disabled="farmerform.$waiting" class="btn btn-primary editable-table-button btn-xs">
										          Save
										        </button>
										        <button type="button" ng-disabled="productform.$waiting" ng-click="productform.$cancel()" class="btn btn-default editable-table-button btn-xs">
										          Cancel
										        </button>
										      </form>
										      <div class="buttons" ng-show="!productform.$visible">
										        <button class="btn btn-primary editable-table-button btn-xs" ng-click="productform.$show()">Edit</button>
										        <button class="btn btn-danger editable-table-button btn-xs" ng-click="removeProduct(product.p_id)">Delete</button>
										      </div>
												</td>

											</tr>
										</tbody>
										<tfoot>
											<tr>
												<th>Product Id</th>
												<th>Product Name</th>
												<th>Category Id</th>
												<th>Price</th>
												<th>Weight</th>
												<th>Average Ratings</th>
												<th>Product by Farmer</th>
												<th>Product Info</th>
												<th>Unit</th>
												<th>Updated At</th>
											</tr>
										</tfoot>
									</table>
								</div><!-- /.box-body -->
							</div><!-- /.box -->
						</div><!-- /.col -->
					</div><!-- /.row -->
				</section><!-- /.content -->
			</div><!-- /.content-wrapper -->
			<footer class="main-footer">
				<% include ./partials/admin-footer %>
			</footer>
			<% include ./partials/admin-aside %>
